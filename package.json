{
	"name": "pyprojectfolders",
	"displayName": "Python Project Folders",
	"description": "This extension allows you to create a Python project folder structure. Based on 'Python Project Template' by raghavwastaken1 https://marketplace.visualstudio.com/items?itemName=raghavwastaken1.pythonprojecttemplate. Some of the changes include additional folders and bash scripts to create a virtual environment.",
	"version": "0.1.0",
	"publisher": "lundeen-bryan",
	"author": "Bryan Lundeen",
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Other",
		"Programming Languages",
		"Snippets"
	],
	"activationEvents": [
		"onCommand:pythonprojecttemplate.createPythonProject"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "pythonprojecttemplate.createPythonProject",
				"title": "Create Python Project"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.5.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.2"
	}
}